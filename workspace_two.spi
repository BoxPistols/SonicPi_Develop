live_loop :midi_piano do
 use_real_time
  note, velocity = sync "/midi/nanokey_keyboard/1/note_on"
   synth :piano, note: note, amp: velocity / 127.0
end


with_fx :reverb, room: 1 do
  live_audio :moog
end

live_loop :moog_trigger do
  use_real_time
  midi (octs :e1, 3).tick, sustain: 0.1
  sleep 0.125
end